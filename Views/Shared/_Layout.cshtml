<!DOCTYPE html>
<html lang="en">

<head>
    @RenderSection("Head", required: true)
</head>

<body class="nav-md">
        <div class="container body">
            <div class="main_container">
                <div class="col-md-3 left_col">
                    <div class="left_col scroll-view">
                        <div class="navbar nav_title" style="border: 0;">
                            <a href="@Url.Action("Index", "Home")" class="site_title">
                                <i class="fa fa-graduation-cap"></i>
                                <span>Exam System Admin</span>
                            </a>
                        </div>

                        <div class="clearfix"></div>

                        <!-- menu profile quick info -->
                        @await Html.PartialAsync("MenuProfileQuickInfo")
                        <!-- /menu profile quick info -->

                        <br/>

                        <!-- sidebar menu -->
                        @await Html.PartialAsync("SidebarMenu")
                        <!-- /sidebar menu -->
                        <!-- /menu footer buttons -->
                        @await Html.PartialAsync("MenuFooterButtons")
                        <!-- /menu footer buttons -->
                    </div>
                </div>

                <!-- top navigation -->
                @await Html.PartialAsync("TopNav")
                <!-- /top navigation -->
                @RenderSection("content", required: true)

                <!-- footer content -->
                @await Html.PartialAsync("footer")
                <!-- /footer content -->
            </div>
        </div>
  
    @RenderSection("JavaLib", required: true)
    @RenderSection("Scripts", required: false)

</body>
</html>
